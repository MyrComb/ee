end)

local Tab = Window:NewTab("Teleports")
local Section = Tab:NewSection("YOU CAN GET KICKED IF U SIT DURING TP")
Section:NewLabel("Good Guns")

Section:NewButton("Revolver", "TP's to Revolver", function()
    local New_CFrame = CFrame.new(-2569.18, 401, -778.763)

local ts = game:GetService('TweenService')
local char = game.Players.LocalPlayer.Character

local part = char.HumanoidRootPart
local ti = TweenInfo.new(3, Enum.EasingStyle.Linear)
local tp = {CFrame = New_CFrame}
ts:Create(part, ti, tp):Play()
wait(4)
fireclickdetector(game.Workspace.Shop.Guns["[Revolver] - $1600"].ClickDetector)
end)

