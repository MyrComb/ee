local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Scratchion                                            discord.gg/xKc62CYcAu", "Ocean")

setclipboard("discord.gg/xKc62CYcAu")

wait (0.2)

game.StarterGui:SetCore("SendNotification", {
    Title = "Discord Invite";
    Text = "Copied Scratchion Invite to your clipboard!";
    Icon = "http://www.roblox.com/asset/?id=8825637242";
    Duration = "3";
})

